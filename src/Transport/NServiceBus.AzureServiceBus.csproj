<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <AssemblyName>NServiceBus.Azure.Transports.WindowsAzureServiceBus</AssemblyName>
    <NuSpecFileName>nservicebus.azureservicebus</NuSpecFileName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SolutionDir)NServiceBus.snk</AssemblyOriginatorKeyFile>
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <OutputPath>..\..\binaries\</OutputPath>
    <NoWarn>CS0419</NoWarn>
    <UpdateAssemblyInfo>true</UpdateAssemblyInfo>
    <NoWarn>1591, 0618</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\packaging\nuget\nservicebus.azureservicebus.nuspec" Link="nservicebus.azureservicebus.nuspec" />
    <PackageReference Include="Janitor.Fody" Version="1.4.1" />
    <PackageReference Include="WindowsAzure.ServiceBus" Version="4.0.0" />
    <PackageReference Include="Microsoft.WindowsAzure.ConfigurationManager" Version="3.2.3" />
    <PackageReference Include="NServiceBus" Version="6.0.0" />
    <PackageReference Include="Obsolete.Fody" Version="4.2.2" />
    <PackageReference Include="Fody" Version="2.1.0" />
    <PackageReference Include="Particular.CodeRules" Version="0.2.0" />
    <PackageReference Include="NuGetPackager" Version="0.6.3" />
    <PackageReference Include="GitVersionTask" Version="4.0.0-beta0012" />
    <Reference Include="System.Configuration" />
    <Reference Include="System.Transactions" />
  </ItemGroup>
</Project>